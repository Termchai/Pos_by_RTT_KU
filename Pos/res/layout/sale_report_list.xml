<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/sale_report_dateTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Date"
            android:textColor="#000000" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " />

        <TextView
            android:id="@+id/sale_report_timeTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Time"
            android:textColor="#000000" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <TextView
            android:id="@+id/sale_report_productTextView"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="ProCode"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/testnaeiei"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="         " />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="Quantity:"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/sale_report_quantityTextView"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="Quan"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="Price:"
            android:textColor="#000000"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/sale_report_priceTextView"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="Price"
            android:textColor="#000000"
            android:textSize="20sp" />

    </LinearLayout>

</LinearLayout>
